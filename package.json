{
    "name": "top-profe",
    "version": "0.0.1",
    "description": "Teaching review tool based on crowdsourcing.",
    "main": "src/server/server.js",
    "scripts": {
        "build": "webpack --mode production",
        "start": "yarn run build && node src/server/server.js",
        "server": "nodemon --ignore dist/ --ignore src/client",
        "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
        "dev": "NODE_ENV=development npx concurrently --kill-others --raw --default-input-target 1 'npm run client' 'npm run server'",
        "test": "eslint . --ext .js,.jsx --ignore-pattern dist/**",
        "db:migrate": "sequelize db:migrate",
        "db:rollback": "sequelize db:migrate:undo",
        "db:drop": "sequelize db:migrate:undo:all",
        "db:seed": "sequelize-cli db:seed:all",
        "db:unseed": "sequelize-cli db:seed:undo"
    },
    "repository": "https://github.com/DAT-ETSIT/top-profe",
    "author": "Delegación de Alumnos de Telecomunicación",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^0.1.19",
        "concurrently": "^4.0.1",
        "css-loader": "^1.0.0",
        "eslint": "^5.7.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "nodemon": "^1.18.4",
        "style-loader": "^0.23.1",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.6",
        "@fortawesome/free-regular-svg-icons": "^5.4.1",
        "@fortawesome/free-solid-svg-icons": "^5.4.1",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "express": "^4.16.4",
        "connect-session-sequelize": "^7.1.7",
        "express-session": "^1.15.6",
        "flow-bin": "^0.85.0",
        "mariadb": "^3.3.0",
        "morgan": "^1.9.1",
        "promise-mysql": "^3.3.1",
        "react": "^16.8",
        "react-dom": "^16.8",
        "react-rating": "^1.4.1",
        "react-router-dom": "^4.3.1",
        "react-search-input": "^0.11.3",
        "sequelize": "^6.37.2",
        "sequelize-cli": "^6.6.2",
        "openid-client": "^5.6.5",
        "passport": "^0.7.0",
        "helmet": "^7.1.0",
        "cookie-parser": "^1.4.6"
        }
}
